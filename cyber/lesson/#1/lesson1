prices = {"bamba":10, "apple":12, "water":9, "peas":8, "egg":1}
shopping_cart = {"bamba":2, "apple":4, "water":11, "peas":12, "egg":3, "orange":10}

sum = 0
for (name, amount) in shopping_cart.items():
    if(name in prices.keys()):
        sum += prices[name]*amount
    else:
        print("error ", name, " not in prices")
print("sum = ", sum)